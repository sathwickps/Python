   "source": [
    "list=[]\n",
    "n=int(input(\"Enter the size of list \"))\n",
    "for i in range(1,n+1):\n",
    "    ele=int(input(\"Enter the element \"))\n",
    "    list.append(ele)\n",
    "sume=0\n",
    "sumo=0\n",
    "for i in list:\n",
    "    if(i%2==0):\n",
    "        sume+=i\n",
    "    else:\n",
    "        sumo+=i\n",
    "print(\"Even Sum = \",sume)\n",
    "print(\"Odd Sum = \",sumo)"
   ]
